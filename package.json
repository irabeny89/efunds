{
  "name": "efunds",
  "version": "1.0.0",
  "description": "efund allow transfer of funds between users.",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only --rs ./src/index.ts",
    "build": "tsc --outDir ./dist/ --target es2016 --esModuleInterop true --module commonjs ./src/index.ts",
    "test": "cross-env NODE_ENV=test jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "funds",
    "credit",
    "debit",
    "transfer",
    "withdraw"
  ],
  "author": "Ernest Irabor",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3",
    "knex": "^1.0.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "@types/express": "^4.17.13",
    "@types/mysql": "^2.15.21",
    "@types/node": "^15.14.9",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "supertest": "^6.2.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
